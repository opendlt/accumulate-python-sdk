[run]
branch = True
concurrency = thread
source = src/accumulate_client
omit =
    */tests/*
    */test_*
    */__init__.py
    */__main__.py
    */examples/*
    */scripts/*
    */conftest.py
    */setup.py

[report]
show_missing = True
precision = 1
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

ignore_errors = True

[html]
directory = htmlcov
title = Accumulate Python SDK Coverage Report